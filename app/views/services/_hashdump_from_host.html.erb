
<% if HashDump.all.empty? %>
  <ul>
    <li> <b>No Hashdump-Data available</b>
  </ul>
<% else %>

<div class="divTable">
  <div class="rowContent">
    <div class="cell" style="padding-right:15px;">
      <h3>Hashdump-Data</h3>
    </div>
    <div class="cell">
      <form name="example" action="#" onsubmit='hideElement("hash"); return false'>
          <input type="submit" name="submit" value="+"/>
      </form>
    </div>
  </div>
</div>

  <table id="hash" style="display:none">
  <tr>
    <th>Hashkey</th>
  </tr>
  <% HashDump.all.each do |tupel| %>
    <tr>
      <td><%= tupel.hash_key %></td>
   </tr>
  <% end %>
  </table>
<% end %>
