
<% if FrfxForm.all.empty? %>
  <ul>
    <li> <b>No Firefox-Form-Data available</b>
  </ul>
<% else %>

<div class="divTable">
  <div class="rowContent">
    <div class="cell" style="padding-right:15px;">
      <h3>Firefox-Formular-Data</h3>
    </div>
    <div class="cell">
      <form name="example" action="#" onsubmit='hideElement("frfx"); return false'>
          <input type="submit" name="submit" value="+"/>
      </form>
    </div>
  </div>
</div>

  <table id="frfx" style="display:none">
  <tr>
    <th>Formular name</th>
    <th>Value</th>
    <th>Updated at</th>
  </tr>
  <% FrfxForm.all.each do |tupel| %>
    <tr>
      <td><%= tupel.form_name %></td>
      <td><%= tupel.value %></td>
      <td><%= tupel.updated_at%></td>
   </tr>
  <% end %>
  </table>
<% end %>
