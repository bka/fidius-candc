<div id="scan-dialog" title="Scan">
	<p>Please enter range.</p>
  <% form_tag "actions/scan",:remote=>true,:onsubmit=>"$('#scan-dialog').dialog('close');" do |form| %>
    <%= text_field :tf,:iprange %>
    <%= submit_tag %>
  <% end %>
</div>

<script>
  $("#scan-dialog").dialog({ autoOpen: false, width:800});
</script>
