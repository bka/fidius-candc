<h2>Sessions</h2>
<% if sessions.empty? %>
<p>No sessions available</p>
<% else %>
<table border="1">
<tr>
  <th>Exploit name</th>
  <th>Payload name</th>
  <th>Exploited at</th>
  <th>Service</th>
</tr>
<% sessions.each do |session| %>
<tr>
  <td><%= session.name %></td>
  <td><%= session.payload %></td>
  <td><%= session.created_at %></td>
  <td><%= session.service.name %></td>
</tr>
<% end %>
</table>
<% end %>
